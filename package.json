{
    "name": "electron-arduino-quickstart",
    "version": "1.0.0",
    "description": "Boilerplate for getting started with Arduino and Electron",
    "main": "window.js",
    "scripts": {
        "postinstall": "./node_modules/.bin/electron-rebuild -v $(./node_modules/.bin/electron -v)",
        "start": "./node_modules/.bin/electron .",
        "watch": "./node_modules/.bin/electron . --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/leoweigand/electron-arduino-quickstart.git"
    },
    "author": "Leo Weigand",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/leoweigand/electron-arduino-quickstart/issues"
    },
    "homepage": "https://github.com/leoweigand/electron-arduino-quickstart#readme",
    "dependencies": {
        "chart.js": "^2.7.3",
        "johnny-five": "^0.10.13",
        "serialport": "^7.0.2"
    },
    "devDependencies": {
        "electron": "^3.0.10",
        "electron-rebuild": "^1.8.2",
        "electron-reload": "^1.2.1",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-sass-lint": "^1.4.0",
        "node-sass": "^4.10.0"
    }
